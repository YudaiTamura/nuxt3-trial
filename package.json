{
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "husky install && nuxt prepare",
    "typecheck": "nuxt typecheck",
    "lint": "eslint --ext .ts,.js,.vue ."
